<h1>Contests List</h1>
<a href="/contests/create" class="badge badge-success">Create New Contest</a>
<hr>
<table class="table table-striped">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Code</th>
            <th scope="col">Name</th>
            <th scope="col">Start</th>
            <th scope="col">End</th>
            <th scope="col">status</th>
        </tr>
    </thead>
    <tbody>
        <% list.forEach(function(contest){ %>
        <tr>
            <td><a href="<%=contest.url%>"><%=contest.code %></a></td>
            <td><%=contest.name %></td>
            <td><%=contest.start_time_formatted %></td>
            <td><%=contest.end_time_formatted %></td>
            <td><%=contest.status %></td>
        </tr>
        <%});%>
    </tbody>
</table>